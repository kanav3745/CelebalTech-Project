use Celebel;

SELECT Start_Date,MIN(End_Date) as End_Date FROM
(SELECT Start_Date FROM Projects WHERE Start_Date NOT IN (SELECT End_Date FROM Projects)) AS s,
(SELECT Start_Date From Projects WHERE Start_Date NOT IN (SELECT Start_Date FROM Projects)) AS e,
WHERE Start_Date < End_Date
GROUP BY Start_Date
ORDER BY DATEDIFF (DAY,MIN(e.Proj_End_Date,Start_Date),Start_Date;
